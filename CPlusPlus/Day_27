// 30DaysOfCode_Day27.cpp : Defines the entry point for the console application.

#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <cstdio>
#include <iostream>
using namespace std;

int main()
{
	cout << "5" << "\n";

	cout << "4 3" << "\n";
	cout << "-1 0 4 2" << "\n";

	cout << "5 3" << "\n";
	cout << "-3 -2 -5 4 1" << "\n";

	cout << "7 3" << "\n";
	cout << "-3 -2 5 4 1 2 5" << "\n";

	cout << "3 3" << "\n";
	cout << "-1 -2 3 -4" << "\n";

	cout << "6 3" << "\n";
	cout << "-3 -2 3 1 1 2" << "\n";

	cin.get();
	return 0;
}
